@model EntradaSalida
@{
    ViewData["Title"] = "Registrar Salida";
}
<h3> Estas seguro que deseas registar la salida de auto con placas @Model.Placas?</h3>

<form asp-action="CrearSalida">

    <input type="hidden" asp-for="@Model.Id" />

    <button type="submit" class="btn btn-danger">Registrar Salida </button>
    <a class="btn btn-secondary" asp-action="IndexEntradasSalidas">Cancelar</a>
</form>